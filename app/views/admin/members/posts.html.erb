<div class="container mt-5">
  <div class="row">
    <div class="col-md-8 mx-auto">
      <h2 class="mb-5"><%= @member.name %>さんの作品一覧</h2>
      <% if @posts.exists? %>
        <%= render 'admin/posts/index', posts: @posts %>
      <% else %>
        <h5 class="text-center mt-5">作品は投稿されていません</h5>
      <% end %>
    </div>
  </div>
</div>